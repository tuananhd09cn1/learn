public class AppTest{

	private int counter;

	public void printIt(){
		System.out.println("printIt() no param");
	}

	public List<String> printItString(String temp){
		List<String> list = new ArrayList<String>();
		list.add(new String("ABC"));
		list.add(new String("DEF"));
		return list;
	}

	public void printItInt(int temp){
		System.out.println("printIt() with param int : " + temp);
	}

	public void setCounter(int counter){
		this.counter = counter;
		System.out.println("setCounter() set counter to : " + counter);
	}

	public void printCounter(){
		System.out.println("printCounter() : " + this.counter);
	}
}


----------------------------------------
	public List<String> loadCLass(){
		//no paramater
		Class noparams[] = {};

		//String parameter
		Class[] paramString = new Class[1];
		paramString[0] = String.class;

		//int parameter
		Class[] paramInt = new Class[1];
		paramInt[0] = Integer.TYPE;
		List<String> test;
		try{
		    //load the AppTest at runtime
			Class cls = Class.forName("com.samsung.svmc.pads.config.AppTest");
			Object obj = cls.newInstance();
			//call the printItString method, pass a String param
			Method method = cls.getDeclaredMethod("printItString", paramString);
			 test = (List<String>)method.invoke(obj, new String("mkyong"));

	
			return test;

		}catch(Exception ex){
			ex.printStackTrace();
		}
		return null;
	}
